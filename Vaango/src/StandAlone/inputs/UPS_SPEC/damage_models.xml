<?xml version='1.0' encoding='ISO-8859-1' ?>
<Uintah_Include>

  <failure_criterion                    spec="OPTIONAL STRING 'MaximumPrincipalStress, MaximumPrincipalStrain, MohrCoulomb'" /> 

  <!-- The following three are for UCNH with failureCriteria of MohrColoumb-->
  <tensile_cutoff_fraction_of_cohesion spec="OPTIONAL DOUBLE" /> <!-- Range from 0 to infinity -->
  <friction_angle                      spec="OPTIONAL DOUBLE" /> <!-- In degrees -->
  <failure_mean                        spec="OPTIONAL DOUBLE" />
  <failure_std                         spec="OPTIONAL DOUBLE" />
  <failure_distrib                     spec="OPTIONAL STRING 'constant, gauss, weibull, uniform'" />
  <scaling                             spec="OPTIONAL STRING 'none, kayenta'"/>
  <exponent                            spec="OPTIONAL DOUBLE" />
  <failure_seed                        spec="OPTIONAL INTEGER" />
  <char_time                           spec="OPTIONAL DOUBLE" />

  <!-- BrittleDamage in UCNH for cnh_damage -->
  <brittle_damage_modulus              spec="OPTIONAL DOUBLE" />
  <brittle_damage_initial_threshold    spec="OPTIONAL DOUBLE" />
  <brittle_damage_fracture_energy      spec="OPTIONAL DOUBLE" />
  <brittle_damage_constant_D           spec="OPTIONAL DOUBLE" />
  <brittle_damage_max_damage_increment spec="OPTIONAL DOUBLE" />
  <brittle_damage_allowRecovery        spec="OPTIONAL BOOLEAN" />
  <brittle_damage_recoveryCoeff        spec="OPTIONAL DOUBLE" />
  <brittle_damage_printDamage          spec="OPTIONAL BOOLEAN" />

</Uintah_Include>

